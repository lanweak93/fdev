    <select id="queryWorkNoBynuitNo" resultMap= "WorkOrder">
        SELECT
        work_no
        FROM
        work_order
        where
        work_stage!=11 and
        work_unit=#{unitNo}
    </select>

    <update id="synDemandUpdateOrderInfo" >
        update
        work_order
        set
        demand_name = #{name}
        where
        demand_id = #{demandId}
    </update>
</mapper>